export interface ApiResponse<T> {
  data: {
    data: T[];
    message: string;
  };
}

export interface MappingDetail {
  id: string;
  code: string;
  name: string;
  status: number;
}

export interface PackageTierMapping {
  packageCode: string;
  packageName: string;
  billingCycle: string;
  price: string;
  allowsTrial: boolean;
  trialDays: number | null;
  details: MappingDetail[];
}

export interface PackageTierMappingApiResponse {
  status: number;
  message: string;
  data: {
    data: PackageTierMapping[];
  };
}